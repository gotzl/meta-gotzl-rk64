proc                 /proc                proc       defaults              0  0
devpts               /dev/pts             devpts     mode=0620,gid=5       0  0
tmpfs                /run                 tmpfs      mode=0755,nodev,nosuid,strictatime 0  0
tmpfs                /var/volatile        tmpfs      defaults              0  0

/dev/zram0           none                 swap       noauto,sw,pri=5       0  0
/dev/nvme0n1         /nvme                ext4       defaults,async,noauto,noatime 0  0
/dev/mmcblk0p1       /media/card          ext4       defaults,sync,noauto  0  0
/dev/disk/by-label/stuff            /srv                 auto       defaults,async,noauto,noatime  0  0
/dev/disk/by-label/vault            /vault                 auto       defaults,async,noauto  0  0

